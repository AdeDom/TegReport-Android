<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adedom.tegreport">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TegReport">
        <activity android:name=".presentation.multiitem.MultiItemActivity" />
        <activity android:name=".presentation.singleitem.SingleItemActivity" />
        <activity android:name=".presentation.multicollection.MultiCollectionActivity" />
        <activity android:name=".presentation.itemcollection.ItemCollectionActivity" />
        <activity android:name=".presentation.room.RoomActivity" />
        <activity android:name=".presentation.logactive.LogActiveActivity" />
        <activity android:name=".presentation.player.PlayerActivity" />
        <activity android:name=".presentation.testfinal.FinalActivity" />
        <activity android:name=".presentation.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>